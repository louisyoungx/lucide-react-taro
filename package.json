{
  "name": "lucide-react-taro",
  "version": "1.2.0",
  "description": "Lucide icons for Taro with dynamic color support, tree shaking, and WeChat miniprogram compatibility",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "taro-lucide-tabbar": "./dist/cjs/cli/index.js"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./icons/*": {
      "types": "./dist/types/icons/*.d.ts",
      "import": "./dist/esm/icons/*.js",
      "require": "./dist/cjs/icons/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fetch-icons": "tsx scripts/fetch-icons.ts",
    "generate": "tsx scripts/generate.ts",
    "build:icons": "npm run fetch-icons && npm run generate",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:bundle": "tsx scripts/build.ts",
    "build": "npm run build:icons && npm run build:bundle && npm run build:cli",
    "clean": "rm -rf dist src/icons",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "lucide",
    "icons",
    "taro",
    "react",
    "miniprogram",
    "wechat",
    "weixin",
    "svg",
    "icon",
    "tree-shaking"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/louisyoungx/lucide-react-taro.git"
  },
  "homepage": "https://github.com/louisyoungx/lucide-react-taro#readme",
  "bugs": {
    "url": "https://github.com/louisyoungx/lucide-react-taro/issues"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3.0.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@tarojs/components": "4.1.9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.3",
    "jsdom": "^28.0.0",
    "react": "^18.0.0",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.5",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "sharp": "^0.33.0"
  }
}