{
  "name": "lucide-react-taro",
  "version": "1.0.1",
  "description": "Lucide icons for Taro with dynamic color support, tree shaking, and WeChat miniprogram compatibility",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/esm/icons/*.js",
      "require": "./dist/cjs/icons/*.js",
      "types": "./dist/types/icons/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fetch-icons": "tsx scripts/fetch-icons.ts",
    "generate": "tsx scripts/generate.ts",
    "build:icons": "npm run fetch-icons && npm run generate",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:icons && npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist src/icons",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "lucide",
    "icons",
    "taro",
    "react",
    "miniprogram",
    "wechat",
    "weixin",
    "svg",
    "icon",
    "tree-shaking"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "peerDependencies": {
    "@tarojs/components": ">=3.0.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@tarojs/components": "4.1.9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.0.0",
    "react": "^18.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.5",
    "vitest": "^4.0.18"
  }
}
